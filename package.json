{
  "name": "regelspraak",
  "version": "1.0.0",
  "description": "TypeScript RegelSpraak DSL parser and rules engine",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "regelspraak": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./server": {
      "require": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts"
    }
  },
  "scripts": {
    "prebuild": "node scripts/extract-multiword.js",
    "build": "tsc && chmod +x dist/cli.js",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist .tsbuildinfo",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "regelspraak",
    "parser",
    "dsl",
    "dutch",
    "business-rules",
    "rules-engine",
    "antlr4"
  ],
  "author": "Yeb Havinga <yhavinga@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yhavinga/regelspraak-ts.git"
  },
  "dependencies": {
    "antlr4": "^4.13.2",
    "decimal.js": "^10.4.3"
  },
  "optionalDependencies": {
    "express": "^4.21.2"
  },
  "peerDependencies": {
    "express": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
